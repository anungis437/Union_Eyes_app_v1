#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/APPS/court_lens_app_v1/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_c3fab8f973bb9a404928c5ed8c87efb0/node_modules/ts-jest/node_modules:/mnt/d/APPS/court_lens_app_v1/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_c3fab8f973bb9a404928c5ed8c87efb0/node_modules:/mnt/d/APPS/court_lens_app_v1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/APPS/court_lens_app_v1/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_c3fab8f973bb9a404928c5ed8c87efb0/node_modules/ts-jest/node_modules:/mnt/d/APPS/court_lens_app_v1/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_c3fab8f973bb9a404928c5ed8c87efb0/node_modules:/mnt/d/APPS/court_lens_app_v1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
