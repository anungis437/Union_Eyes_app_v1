{
  "name": "unioneyes",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "turbo run build",
    "build:next": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "start coverage/index.html",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run __tests__/lib __tests__/services",
    "test:unit:coverage": "vitest run __tests__/lib __tests__/services --coverage",
    "test:integration": "vitest run __tests__/integration",
    "test:integration:coverage": "vitest run __tests__/integration --coverage",
    "test:performance": "vitest run __tests__/performance/",
    "test:perf:graphql": "vitest run __tests__/performance/graphql-api-performance.test.ts",
    "test:perf:db": "vitest run __tests__/performance/database-performance.test.ts",
    "test:perf:pension": "vitest run __tests__/performance/pension-processor-performance.test.ts",
    "test:perf:insurance": "vitest run __tests__/performance/insurance-adapter-performance.test.ts",
    "test:perf:concurrent": "vitest run __tests__/performance/concurrent-operations-performance.test.ts",
    "test:perf:verbose": "vitest run __tests__/performance/ --reporter=verbose",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "type-check": "tsc --noEmit --incremental",
    "clean": "rimraf .next node_modules/.cache dist packages/**/dist services/**/dist",
    "repo:hygiene": "node scripts/check-repo-hygiene.js",
    "repo:hygiene:bash": "bash scripts/check-repo-hygiene.sh",
    "validate:api-guards": "node scripts/check-api-guards.js",
    "security:audit-routes": "npx tsx scripts/route-security-audit.ts",
    "coverage:summary": "node -e \"const c=require('./coverage/coverage-summary.json'); console.log('Coverage:', c.total.lines.pct + '%')\"",
    "migration:status": "node -e \"const fs=require('fs'); const routes=fs.readdirSync('app/api',{recursive:true}).filter(f=>f.endsWith('route.ts')); const migrated=routes.filter(r=>{const c=fs.readFileSync('app/api/'+r,'utf8'); return c.includes('standardErrorResponse')}); console.log(`Migration Progress: ${migrated.length}/${routes.length} (${Math.round(migrated.length/routes.length*100)}%)`);\"",
    "openapi:generate": "npx tsx scripts/openapi-generator.ts",
    "openapi:generate:merge": "npx tsx scripts/openapi-generator.ts --merge",
    "openapi:generate:enhanced": "npx tsx scripts/openapi-generator-enhanced.ts",
    "openapi:add-examples": "npx tsx scripts/add-api-examples.ts",
    "openapi:complete": "pnpm run openapi:generate:enhanced && pnpm run openapi:add-examples",
    "openapi:watch": "nodemon --watch app/api --ext ts --exec \"pnpm openapi:generate\"",
    "schema:consolidate": "npx tsx scripts/schema-consolidation/consolidate-chart-of-accounts.ts",
    "schema:consolidate:dry-run": "npx tsx scripts/schema-consolidation/consolidate-chart-of-accounts.ts --dry-run",
    "schema:consolidate:verbose": "npx tsx scripts/schema-consolidation/consolidate-chart-of-accounts.ts --dry-run --verbose",
    "video": "remotion studio",
    "render": "remotion render src/remotion/index.tsx ShowcaseVideo out/video.mp4",
    "seed:platform": "npx tsx scripts/seed-full-platform.ts",
    "seed:claims": "npx tsx scripts/seed-test-claims.ts",
    "seed:historical": "npx tsx scripts/seed-historical-claims.ts",
    "seed:clc-accounts": "npx tsx scripts/seed-clc-accounts.ts",
    "ml:retrain": "npx tsx scripts/ml-retraining-pipeline.ts",
    "ml:retrain:all": "npx tsx scripts/ml-retraining-pipeline.ts",
    "ml:train:churn": "npx tsx scripts/train-churn-model.ts",
    "ml:train:workload": "npx tsx scripts/train-workload-forecast-model.ts",
    "validate:blind-spots": "npx tsx scripts/run-validators.ts",
    "validate:blind-spots:privacy": "npx tsx scripts/run-validators.ts --category=privacy",
    "validate:blind-spots:critical": "npx tsx scripts/run-validators.ts --only=1,2,3,4",
    "encrypt:generate-key": "npx tsx scripts/generate-encryption-key.ts",
    "encrypt:test": "npx tsx scripts/test-encryption-system.ts",
    "encrypt:migrate": "npx tsx scripts/migrate-sin-to-encrypted.ts",
    "encrypt:migrate:dry-run": "npx tsx scripts/migrate-sin-to-encrypted.ts --dry-run",
    "schema:analyze-deps": "npx tsx scripts/schema-consolidation/analyze-schema-deps.ts",
    "schema:find-duplicates": "npx tsx scripts/schema-consolidation/find-duplicates.ts",
    "schema:map-domains": "npx tsx scripts/schema-consolidation/map-domains.ts",
    "schema:analyze-all": "pnpm run schema:analyze-deps && pnpm run schema:find-duplicates && pnpm run schema:map-domains",
    "schema:migrate-imports": "npx tsx scripts/schema-consolidation/migrate-imports.ts",
    "schema:migrate-imports:dry-run": "npx tsx scripts/schema-consolidation/migrate-imports.ts --dry-run",
    "schema:drift:detect": "npx tsx scripts/schema-drift-detector.ts",
    "schema:drift:detect:json": "npx tsx scripts/schema-drift-detector.ts --json",
    "schema:drift:validate": "npx tsx scripts/validate-migrations.ts",
    "schema:drift:validate:json": "npx tsx scripts/validate-migrations.ts --json",
    "schema:drift:compare": "node scripts/compare-schemas.js",
    "schema:drift:check-all": "pnpm run schema:drift:detect && pnpm run schema:drift:validate",
    "schema:index:analyze": "npx tsx scripts/analyze-missing-indexes.ts",
    "schema:index:analyze:json": "npx tsx scripts/analyze-missing-indexes.ts --json",
    "compliance:deploy:phase1": "pwsh scripts/compliance/deploy-compliance-implementation.ps1 -Phase 1",
    "compliance:deploy:phase2": "pwsh scripts/compliance/deploy-compliance-implementation.ps1 -Phase 2",
    "compliance:deploy:phase3": "pwsh scripts/compliance/deploy-compliance-implementation.ps1 -Phase 3",
    "compliance:deploy:phase4": "pwsh scripts/compliance/deploy-compliance-implementation.ps1 -Phase 4",
    "compliance:deploy:all": "pwsh scripts/compliance/deploy-compliance-implementation.ps1 -Phase all",
    "compliance:deploy:phase1:dry": "pwsh scripts/compliance/deploy-compliance-implementation.ps1 -Phase 1 -DryRun",
    "compliance:deploy:phase2:dry": "pwsh scripts/compliance/deploy-compliance-implementation.ps1 -Phase 2 -DryRun",
    "compliance:test:all": "vitest run __tests__/compliance/",
    "compliance:test:pci": "vitest run __tests__/compliance/pci*.test.ts",
    "compliance:test:aml": "vitest run __tests__/compliance/aml*.test.ts",
    "compliance:test:iso": "vitest run __tests__/compliance/iso*.test.ts",
    "compliance:report:pci": "npx tsx scripts/compliance/generate-pci-report.ts",
    "compliance:report:aml": "npx tsx scripts/compliance/generate-aml-report.ts",
    "compliance:report:iso": "npx tsx scripts/compliance/generate-iso-report.ts",
    "compliance:status": "npx tsx scripts/compliance/compliance-status.ts",
    "compliance:sbom": "npx tsx scripts/compliance/generate-sbom.ts",
    "compliance:evidence": "npx tsx scripts/compliance/generate-evidence-bundle.ts",
    "compliance:evidence:full": "npx tsx scripts/compliance/generate-evidence-bundle.ts --include-audit-sample",
    "compliance:audit": "pnpm compliance:sbom && pnpm compliance:evidence:full",
    "prepare": "husky"
  },
